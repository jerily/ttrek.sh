{
	"name": "glibc",
	"version": "2.39.0",
	"scripts": {
	},
	"dependencies":	{
	},
	"devDependencies": {
	},
	"build": {
		"default": [
			{ "cmd": "download", "url": "https://ftp.gnu.org/gnu/glibc/glibc-2.39.tar.gz" },
			{ "cmd": "unpack" },
			{ "cmd": "cd" },
			{
				"cmd": "configure",
				"options": [
					{ "name": "prefix", "value": "$INSTALL_DIR" }
				]
			},
			{ "cmd": "make" },
			{ "cmd": "make_install", "no_ld_library_path": true }
		]
	}
}