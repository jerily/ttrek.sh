{
	"name":	"curl",
	"version":	"8.7.1",
	"scripts":	{
	},
	"dependencies":	{
		"openssl":	"^3.0.13",
		"zlib":	"^1.3.1"
	},
	"devDependencies":	{
	},
	"build": {
		"linux.x86_64": [
			{ "cmd": "download", "url": "https://curl.se/download/curl-8.7.1.tar.gz" },
			{ "cmd": "unpack" },
			{ "cmd": "cd" },
			{
				"cmd": "configure",
				"options": [
					{ "name": "with-openssl", "value": "$INSTALL_DIR" },
					{ "name": "with-zlib", "value": "$INSTALL_DIR" }
				]
			},
			{ "cmd": "make" },
			{ "cmd": "install" }
		]
	}
}