{
	"name":	"tjson",
	"version":	"1.0.14",
	"scripts":	{
	},
	"dependencies":	{
		"tcl": "^9.0.0-beta.2"
	},
	"devDependencies":	{
	},
	"build": {
		"linux.x86_64": [
			{ "cmd": "download", "url": "https://github.com/jerily/tjson/archive/refs/tags/v1.0.14.tar.gz" },
			{ "cmd": "unpack" },
			{ "cmd": "cd" },
			{ "cmd": "cmake_config",
				"options": [
					{"name": "TCL_LIBRARY_DIR", "value":  "$INSTALL_DIR/lib"},
					{"name": "TCL_INCLUDE_DIR", "value":  "$INSTALL_DIR/include"}
				]
			},
			{ "cmd": "make" },
			{ "cmd": "make_install" }
		]
	}
}