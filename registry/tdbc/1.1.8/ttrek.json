{
	"name":	"tdbc",
	"version":	"1.1.8",
	"scripts":	{
	},
	"dependencies":	{
		"tcl": ">=8.6.14"
	},
	"devDependencies":	{
	},
	"build": {
		"default": [
			{ "cmd": "download", "url": "https://sourceforge.net/projects/tcl/files/Tcl/9.0b2/tdbc1.1.8.tar.gz" },
			{ "cmd": "unpack" },
			{ "cmd": "cd" },
			{
				"cmd": "configure",
				"options": [{"name": "with-tcl", "value": "$INSTALL_DIR/lib"}]
			},
			{ "cmd": "make", "parallel": true },
			{ "cmd": "make_install" }
		]
	}
}