{
	"name":	"tdom",
	"version":	"0.9.4-alpha",
	"scripts":	{
	},
	"dependencies":	{
		"tcl": "^9.0.0-beta.2"
	},
	"devDependencies":	{
	},
	"build": {
		"linux.x86_64": [
			{ "cmd": "download", "url": "http://tdom.org/index.html/tarball/trunk/tdom-trunk.tar.gz" },
			{ "cmd": "unpack" },
			{ "cmd": "cd", "dir": "$PACKAGE_NAME/unix" },
			{
				"cmd": "configure",
				"path": "../configure",
				"options": [{"name": "with-tcl", "value": "$INSTALL_DIR/lib"}]
			},
			{ "cmd": "make" },
			{ "cmd": "install" }
		]
	}
}